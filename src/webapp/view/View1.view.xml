<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:f="sap.ui.layout.form" controllerName="victoria.controller.View1" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="Entries" showHeader="true">
		<headerContent></headerContent>
		<content>
			<!-- <List
			headerText="Entries"
			footerText="Total : 50"
			items="{/Entrys}" >
			<StandardListItem
				title="{RefNo}"
				description="{Name}"
			/>
		</List> -->

			<Table id="idEntryTable" inset="false" items="{
			path: '/Entrys',
			sorter: {
				path: 'Date'
			}
		}" footerText="Total : {local>/TotalRowCount}" mode="SingleSelectMaster" updateFinished="onEntryUpdate" selectionChange="onItemPress">
				<!-- <headerToolbar>
					<OverflowToolbar>
						<content></content>
					</OverflowToolbar>
				</headerToolbar> -->
				<columns>
					<Column>
						<Text text="Ref No." />
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="Name" />
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="Date" />
					</Column>
					<Column minScreenWidth="Desktop" demandPopin="true">
						<Text text="Item" />
					</Column>
					<Column minScreenWidth="Desktop" demandPopin="true" hAlign="Center">
						<Text text="Weight" />
					</Column>
					<Column>
						<Text text="DeliveryBy" />
					</Column>
					<Column>
						<Text text="Pieces" />
					</Column>
					<Column>
						<Text text="Logo" />
					</Column>
					<Column>
						<Text text="HM Rate" />
					</Column>
					<Column>
						<Text text="Market Rate" />
					</Column>
					<Column>
						<Text text="Total" />
					</Column>
					<Column>
						<Text text="Print" />
					</Column>
				</columns>
				<items>
					<ColumnListItem vAlign="Middle">
						<cells>
							<ObjectIdentifier title="{RefNo}" />
							<Text text="{Name}" />
							<Text text="{
								path: 'Date',
								type: 'sap.ui.model.type.Date',
								formatOptions: { style: 'medium' }
							}" />
							<Text text="{Item}" />
							<Text text="{Weight}" />
							<Text text="{DeliveryBy}" />
							<Text text="{TotalPcs}" />
							<Text text="{Logo}" />
							<Text text="{OMRate}" />
							<Text text="{MarketRate}" />
							<Text text="{Total}" />
							<Button icon="sap-icon://print" press="onPrint" />
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
		<footer>
			<Toolbar></Toolbar>
		</footer>
	</Page>
</mvc:View>