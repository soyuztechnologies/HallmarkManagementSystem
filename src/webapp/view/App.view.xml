<mvc:View xmlns:core="sap.ui.core" xmlns:c="sap.ui.commons" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="victoria.controller.App"
	xmlns:html="http://www.w3.org/1999/xhtml" xmlns:f="sap.f" xmlns:semantic="sap.m.semantic" xmlns:pageLayout="sap.uxap" displayBlock="true"
	xmlns:tnt="sap.tnt">
		<tnt:ToolHeader design="Solid" style="Clear" visible="{appView>/headerVisible}">
			<Button icon="sap-icon://home" text="Home" type="Transparent" press="onHomePress">
				<layoutData>
					<OverflowToolbarLayoutData priority="NeverOverflow" />
				</layoutData>
			</Button>
		
			<Button text="New Entry" type="Transparent" press="onNewEntry">
				<layoutData>
					<OverflowToolbarLayoutData priority="NeverOverflow" />
				</layoutData>
			</Button>
		</tnt:ToolHeader>
		<f:FlexibleColumnLayout
			id="layout"
			layout="{appView>/layout}"
			backgroundDesign="Translucent">
		</f:FlexibleColumnLayout>
</mvc:View>
